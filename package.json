{
  "name": "node-rdkafka",
  "version": "v3.1.1",
  "description": "Node.js bindings for librdkafka",
  "librdkafka": "2.5.0",
  "main": "lib/index.js",
  "scripts": {
    "configure": "node-pre-gyp configure",
    "build": "node-pre-gyp build",
    "test": "make test",
    "install": "node-pre-gyp rebuild",
    "prepack": "node ./ci/prepublish.js",
    "publish": "npm run configure && npm run install && node-pre-gyp package && npm run prepack && node-pre-gyp-github publish --release"
  },
  "keywords": [
    "kafka",
    "librdkafka"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:Blizzard/node-rdkafka.git"
  },
  "contributors": [
    {
      "name": "Stephen Parente",
      "email": "webmakersteve@gmail.com"
    },
    {
      "name": "Matt Gollob",
      "email": "mattness@users.noreply.github.com"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "bluebird": "^3.5.3",
    "jsdoc": "^4.0.2",
    "jshint": "^2.10.1",
    "mocha": "^10.2.0",
    "node-gyp": "^10.1.0",
    "toolkit-jsdoc": "^1.0.0"
  },
  "dependencies": {
    "@mapbox/node-pre-gyp": "^1.0.11",
    "bindings": "^1.3.1",
    "nan": "^2.19.0",
    "node-pre-gyp-github": "^2.0.0"
  },
  "engines": {
    "node": ">=16"
  },
  "binary": {
    "module_name": "node-librdkafka",
    "module_path": "./build/Release/",
    "host": "https://github.com/qiujie8092916/node-rdkafka/releases/download/",
    "remote_path": "{version}"
  }
}
